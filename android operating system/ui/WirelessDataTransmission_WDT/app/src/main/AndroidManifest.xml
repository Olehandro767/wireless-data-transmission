<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ua.edu.onaft.wirelessdatatransmission_wdt">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WirelessDataTransmission_WDT">
        <activity android:name=".SplashScreen" android:theme="@style/SplashScreen" android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListOfDevicesActivity" />
        <activity android:name=".FileViewerActivity" />
        <activity android:name=".WelcomeActivity"/>
    </application>

</manifest>